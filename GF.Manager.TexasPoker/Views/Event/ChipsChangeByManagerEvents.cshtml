
@{
    ViewBag.Title = "Events";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div ng-controller="ChipsChangeByManagerEventsController">
    <h2 class="page-header">管理员修改筹码</h2>
    @Html.Partial("Partial/_ListPageHeadBar")
    <table class="table table-bordered table-striped table-condensed">
        <thead>
            <tr>
                <th>玩家EtGuid</th>
                <th>变化筹码数</th>
                <th>变化后筹码总数</th>
                <th>事件时间</th>
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat="row in raws">
                <td>{{row.PlayerEtGuid}}</td>
                <td>{{row.ChangeChips}}</td>
                <td>{{row.AfterChangeChipsNum}}</td>
                <td>{{row.EventTm|date:'yyyy/MM/dd HH:mm'}}</td>
            </tr>
        </tbody>
    </table>
    @Html.Partial("Partial/_ListPageFootBar")
</div>
